<robot name="test_robot">

  <link name="base_link">
    <contact>
      <lateral_friction value="0.0001"/>
      <rolling_friction value="0.0001"/>
      <spinning_friction value="0.0001"/>
    </contact>
    <inertial>
      <mass value="4.54545454545454"/>
      <inertia ixx="0.0103030303030303" ixy="0" ixz="0" iyy="0.0333712121212121" iyz="0" izz="0.024280303030303"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.05 0 0.05"/>
      <geometry>
        <box size="0.25 0.16 0.04"/>
      </geometry>
      <material name="grey888">
        <color rgba=".8 .8 .8 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.05 0 0.05"/>
      <geometry>
        <box size="0.25 0.16 0.04"/>
      </geometry>
      <material name="blue">
        <color rgba=".2 .2 1 1"/>
      </material>
    </collision>
  </link>
  
  <link name="lwheel">
    <inertial>
      <mass value="0.3"/>
      <inertia ixx="0.0001687" ixy="0" ixz="0" iyy="0.0001687" iyz="0" izz="0.0003174"/>
    </inertial>
    <contact>
      <lateral_friction value="0.501"/>
      <rolling_friction value="0.1"/>
      <spinning_friction value="0.002"/>
      <stiffness value="300000"/>
      <damping value="1000"/>
    </contact>
    <visual>
      <geometry>
        <cylinder length="0.02" radius="0.046"/>
      </geometry>
      <origin rpy="0 -1.5707963 0" xyz="0 0 0"/>
      <material name="black112">
        <color rgba=".1 .1 .2 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.02" radius="0.046"/>
      </geometry>
      <origin rpy="0 -1.5707963 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </collision>
  </link>

  <link name="lwheel_mark">
    <inertial>
      <mass value="0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.022 0.08"/>
      </geometry>
      <material name="blue44a">
        <color rgba=".4 .4 1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.022 0.08"/>
      </geometry>
    </collision>
  </link>

  <joint name="lwheel_to_lwheel_mark" type="fixed">
    <parent link="lwheel"/>
    <child link="lwheel_mark"/>
    <origin xyz="0 0 0" rpy="0 0 1.5707963"/>
  </joint>

  <joint name="base_to_lwheel" type="continuous">
    <parent link="base_link"/>
    <child link="lwheel"/>
    <axis xyz="-1 0 0"/>
    <origin xyz="-0.05 0.115 0.046" rpy="0 0 -1.5707963"/>
  </joint>
  
  <link name="rwheel">
    <inertial>
      <mass value="0.3"/>
      <inertia ixx="0.0001687" ixy="0" ixz="0" iyy="0.0001687" iyz="0" izz="0.0003174"/>
    </inertial>
    <contact>
      <lateral_friction value="0.501"/>
      <rolling_friction value="0.1"/>
      <spinning_friction value="0.002"/>
    </contact>
    <visual>
      <geometry>
        <cylinder length="0.02" radius="0.046"/>
      </geometry>
      <origin rpy="0 1.5707963 0" xyz="0 0 0"/>
      <material name="black131">
        <color rgba=".1 .3 .1 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.02" radius="0.046"/>
      </geometry>
      <origin rpy="0 1.5707963 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </collision>
  </link>
  
  <joint name="base_to_rwheel" type="continuous">
    <parent link="base_link"/>
    <child link="rwheel"/>
    <axis xyz="1 0 0"/>
    <origin xyz="-0.05 -0.115 0.046" rpy="0 0 1.5707963"/>
  </joint>
  
  <link name="rwheel_mark">
    <inertial>
      <mass value="0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.022 0.08"/>
      </geometry>
      <material name="green">
        <color rgba="0 1 0 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.022 0.08"/>
      </geometry>
    </collision>
  </link>

  <joint name="rwheel_to_rwheel_mark" type="fixed">
    <parent link="rwheel"/>
    <child link="rwheel_mark"/>
    <origin xyz="0 0 0" rpy="0 0 -1.5707963"/>
  </joint>

  <link name="fwheel_attach">
    <visual>
      <geometry>
        <cylinder length="0.060" radius="0.023"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.13 0 0.08"/>
      <material name="white388">
        <color rgba=".3 .8 .8 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.060" radius="0.023"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0.13 0 0.08"/>
    </collision>
  </link>
  
  <joint name="base_to_fattach" type="fixed">
    <parent link="base_link"/>
    <child link="fwheel_attach"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
  
  <link name="fwheel">
    <inertial>
      <mass value="0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
    <contact>
      <lateral_friction  value="0.0000001"/>
      <rolling_friction  value="0.0000001"/>
      <spinning_friction value="0.0000001"/>
    </contact>
    <visual>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
      <origin rpy="1.5707963 0 0" xyz="0 0 0"/>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.005" radius="0.015"/>
      </geometry>
      <origin rpy="1.5707963 0 0" xyz="0 0 0"/>
    </collision>
  </link>
  
  <joint name="base_to_fwheel" type="fixed">
    <parent link="base_link"/>
    <child link="fwheel"/>
    <origin xyz="0.14 0 0.010" rpy="0 0 0"/>
  </joint>
  
